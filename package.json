{
  "name": "plate-js-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "ts-node --project tsconfig.server.json src/server/index.ts",
    "build": "prisma generate && next build && tsc -p tsconfig.server.json",
    "start": "NODE_ENV=production node -r source-map-support/register dist/server/index.js",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "@trpc/client": "^10.45.0",
    "@trpc/next": "^10.45.0",
    "@trpc/react-query": "^10.45.0",
    "@trpc/server": "^10.45.0",
    "express": "^4.18.2",
    "next": "13.5.6",
    "prisma": "^5.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rfc6902": "^5.1.1",
    "socket.io": "^4.7.4",
    "source-map-support": "^0.5.21",
    "superjson": "^2.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.17.10",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/socket.io": "^3.0.1",
    "@types/source-map-support": "^0.5.10",
    "@types/ws": "^8.5.13",
    "eslint": "^8.56.0",
    "eslint-config-next": "13.5.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
